#include <iostream>
#include <cstdlib>
using namespace std;
/* в функцию в качетсве параметров передаются статический массив mas и его размерности: n - количество строк, m - количество столбцов*/
void print (int mas [3] [2], int n, int m)
{
    for (int i = 0; i < n; ++i, cout << endl)
    for (int j = 0; j < m; ++j)
    cout << mas[i][j] << "\t";
}
int main()
{
    int a[3][2] = {{1, -2}, {-3, 4}, {-5, 6} };
    print(a, 2, 2);
    return 0;
}
