#include <iostream>
#include <cstdlib>
using namespace std;

int main()
{
    int b[10]; // объявление массива
    for (int i = 0; i < 10; i++) // ввод элементов массива
    {
        cout << "b[" << i << "] = ";
        cin >> b[i];
    }
    
    for (int i = 0; i < 10; i++) // вывод элементов массива на экран
    cout << "b[" << i << "] = " << b[i] << "\n";
}
